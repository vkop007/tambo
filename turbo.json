{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalEnv": [
    "ALLOW_LOCAL_MCP_SERVERS",
    "ALLOWED_LOGIN_DOMAIN",
    "API_KEY_SECRET",
    "AUTH_REDIRECT_FROM_HOST",
    "CALENDAR_URL",
    "CI",
    "DATABASE_URL",
    "DISALLOWED_EMAIL_DOMAINS",
    "EMAIL_FROM_DEFAULT",
    "EMAIL_FROM_PERSONAL",
    "EMAIL_REPLY_TO_PERSONAL",
    "EMAIL_REPLY_TO_SUPPORT",
    "EXTRACTION_OPENAI_API_KEY",
    "FALLBACK_OPENAI_API_KEY",
    "FORCE_INTERACTIVE",
    "GITHUB_ACTIONS",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "GITHUB_TOKEN",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "HOST",
    "INKEEP_API_BASE_URL",
    "INKEEP_API_KEY",
    "INTERNAL_SLACK_USER_ID",
    "LANGFUSE_HOST",
    "LANGFUSE_PUBLIC_KEY",
    "LANGFUSE_SECRET_KEY",
    "NEXT_BUILD_STANDALONE",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_CURSOR_MCP_DEEPLINK",
    "NEXT_PUBLIC_DOCS_URL",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SENTRY_ENVIRONMENT",
    "NEXT_PUBLIC_SENTRY_ORG",
    "NEXT_PUBLIC_SENTRY_PROJECT",
    "NEXT_PUBLIC_SMOKETEST_TAMBO_API_KEY",
    "NEXT_PUBLIC_TAMBO_API_KEY",
    "NEXT_PUBLIC_TAMBO_API_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "OPENAI_API_KEY",
    "OPENAPI_SPEC_FILE",
    "PORT",
    "PROVIDER_KEY_SECRET",
    "RESEND_API_KEY",
    "RESEND_AUDIENCE_ID",
    "S3_ACCESS_KEY_ID",
    "S3_BUCKET",
    "S3_ENDPOINT",
    "S3_REGION",
    "S3_SECRET_ACCESS_KEY",
    "SENTRY_DSN",
    "SENTRY_ENVIRONMENT",
    "SKIP_ENV_VALIDATION",
    "SLACK_OAUTH_TOKEN",
    "SLACK_TEAM_ID",
    "SUPABASE_URL",
    "TAMBO_SERVER_URL",
    "TAMBO_WHITELABEL_ORG_LOGO",
    "TAMBO_WHITELABEL_ORG_NAME",
    "TERM",
    "VERCEL_URL",
    "WEATHER_API_KEY",
    "WEBHOOK_SECRET"
  ],
  "globalPassThroughEnv": [
    "TAMBO_REGISTRY_PATH",
    "TAMBO_CLI_CONSOLE_BASE_URL",
    "TAMBO_TOKEN"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "esm/**"],
      "passThroughEnv": ["SENTRY_AUTH_TOKEN"]
    },
    "tambo#build": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "../../packages/ui-registry/src/**/*.ts",
        "../../packages/ui-registry/src/**/*.tsx",
        "../../packages/ui-registry/src/**/*.css",
        "../../packages/ui-registry/src/**/config.json"
      ],
      "outputs": ["dist/**"]
    },
    "@tambo-ai/showcase#build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": { "dependsOn": ["^lint"] },
    "@tambo-ai/ui-registry#lint": {
      "dependsOn": ["@tambo-ai/ui-registry#verify-exports"]
    },
    "verify-exports": {
      "inputs": ["package.json", "src/**", "scripts/**"],
      "outputs": []
    },
    "check-types": { "dependsOn": ["^check-types"] },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "@tambo-ai/react#dev": {
      "cache": false,
      "persistent": true
    },
    "@tambo-ai/showcase#dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean": { "cache": false },
    "start": { "dependsOn": ["^build"] },
    "test": { "dependsOn": ["^test"] },
    "@tambo-ai-cloud/web#test": {
      "dependsOn": ["@tambo-ai/react#build", "^test"]
    }
  }
}
